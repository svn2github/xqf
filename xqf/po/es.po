msgid ""
"Usage:\n"
"\txqf [OPTIONS]\n"
"\n"
"OPTIONS:\n"
"\t--launch \"[SERVERTYPE] IP\"\tlaunch game on specified server\n"
"\t--add    \"[SERVERTYPE] IP\"\tadd specified server to favorites\n"
"\t--debug <level>\t\t\tset debug level\n"
"\t--version\t\t\tprint version and exit\n"
